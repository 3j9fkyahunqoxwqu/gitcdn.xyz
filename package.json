{
  "name": "GitCDN",
  "version": "1.0.0",
  "description": "GitCDN server",
  "keywords": [
    "OpenShift",
    "Node.js",
    "application",
    "openshift"
  ],
  "author": {
    "name": "OpenShift",
    "email": "ramr@example.org",
    "url": "http://www.openshift.com/"
  },
  "homepage": "http://www.openshift.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/openshift/origin-server"
  },
  "engines": {
    "node": ">= 0.6.0",
    "npm": ">= 1.0.0"
  },
  "dependencies": {
    "express": "^4.12.3",
    "request": "^2.55.0",
    "cors": "^2.6.0",
    "pm2": "latest",
    "pmx": "latest",
    "mime": "^1.3.4"
  },
  "devDependencies": {},
  "bundleDependencies": [],
  "private": true,
  "main": "server.js",

  "script"          : "server.js",
  "error_file"        : "err.log",
  "out_file"        : "out.log",
  "merge_logs"      : true,
  "log_date_format" : "YYYY-MM-DD HH:mm Z",
  "max_memory_restart" : "20M",
  "ignore_watch" : ["store-cache"]
}
