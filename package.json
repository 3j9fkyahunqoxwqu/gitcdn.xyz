{
    "name": "GitCDN",
    "version": "1.0.0",
    "description": "GitCDN server",
    "keywords": [
        "OpenShift",
        "Node.js",
        "application",
        "openshift"
    ],
    "author": {
        "name": "GitCDN",
        "email": "schme16@gmail.com",
        "url": "https://gitcdn.xyz"
    },
    "homepage": "https://gitcdn.xyz",
    "repository": {
        "type": "git",
        "url": "git@github.com:schme16/gitcdn.xyz.git"
    },
    "engines": {
        "node": ">= 0.12.6",
        "npm": ">= 1.0.0"
    },
    "dependencies": {
        "express": "^4.12.3",
        "request": "^2.55.0",
        "cors": "^2.6.0",
        "pm2": "latest",
        "pmx": "latest",
        "mime": "^1.3.4"
    },
    "devDependencies": {},
    "bundleDependencies": [],
    "private": true,
    "main": "server.js",
    "apps": [
        {
            "name": "GitCDN",
            "script": "server.js",
            "error_file": "err.log",
            "out_file": "out.log",
            "merge_logs": true,
            "log_date_format": "YYYY-MM-DD HH:mm Z",
            "max_memory_restart": "150M",
            "watch": [
                "server.js",
                "package.json"
            ],
            "ignore_watch": [
                "store-cache"
            ]
        }
    ]
}
